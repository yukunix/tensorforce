{
  "log_level": "info",
  "demo_sampling_ratio": 0.01,
  "supervised_weight": 1.0,
  "expert_margin": 0.8,
  "memory_capacity": 1e4,
  "demo_memory_capacity" : 1e4,
  "batch_size": 32,
  "update_rate": 0.25,
  "update_repeat": 1,
  "use_target_network": true,
  "target_network_update_rate": 0.0001,
  "min_replay_size": 5e2,
  "learning_rate": 0.00025,
  "memory": {
      "type": "replay",
      "random_sampling": true
  },
  "optimizer": {
    "type": "adam",
    "momentum": 0.95,
    "epsilon": 0.01
  },
  "exploration": {
    "type": "epsilon_decay",
    "epsilon": 1,
    "epsilon_final": 0.1,
    "epsilon_states": 1e6
  },
  "preprocessing": [
    {
      "type": "image_resize",
      "width": 84,
      "height": 84
    },
    {
      "type": "grayscale"
    },
    {
      "type": "center"
    },
    {
      "type": "sequence",
      "length": 4
    }
  ],
  "reward_preprocessing": [
    {
      "type": "clip",
      "min": -1,
      "max": 1
    }
  ],
  "repeat_actions": 1,
  "alpha": 0.00025,
  "gamma": 0.99,
  "update_target_weight": 1.0
}
