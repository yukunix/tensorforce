{
  "loglevel": "debug",
  "memory_capacity": 1e4,
  "batch_size": 32,
  "update_rate": 0.25,
  "update_repeat": 1,
  "use_target_network": true,
  "target_update_frequency": 10000,
  "first_update": 5e4,
  "learning_rate": 0.00025,
  "optimizer": "tensorflow.python.training.rmsprop.RMSPropOptimizer",
  "double_dqn": false,
  "optimizer_param": {
    "momentum": 0.95,
    "epsilon": 0.01
  },
  "exploration": "epsilon_decay",
  "exploration_param": {
    "epsilon": 1,
    "epsilon_final": 0.1,
    "epsilon_states": 1e6
  },
  "preprocessing": [
    ["grayscale"],
    ["imresize", 84, 84],
    ["concat", 4, "append"],
    ["normalize"]
  ],
  "repeat_actions": 1
}
