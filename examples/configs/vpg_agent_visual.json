{
  "preprocessing": [
    {
      "type": "image_resize",
      "width": 84,
      "height": 84
    },
    {
      "type": "grayscale"
    },
    {
      "type": "center"
    },
    {
      "type": "sequence",
      "length": 4
    }
  ],
  "exploration": null,

  "batch_size": 4000,

  "discount": 0.97,
