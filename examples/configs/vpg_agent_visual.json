  "tf_summary": null,
  "log_level": "info",

  "baseline": {
    "type": "mlp",
    "size": 100,
    "repeat_update": 100
  },
  "generalized_advantage_estimation": false,
  "gae_lambda": 0.97,
  "normalize_advantage": false
}
