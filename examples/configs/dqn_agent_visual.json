{
  "preprocessing": [
    {
      "type": "image_resize",
      "width": 84,
      "height": 84
    },
    {
      "type": "grayscale"
    },
    {
      "type": "divide",
      "scale": 255
    },
    {
      "type": "sequence",
      "length": 4
    }
  ],
  "exploration": {
    "type": "epsilon_anneal",
    "epsilon": 1.0,
    "epsilon_final": 0.1,
    "epsilon_timesteps": 1e6
  },

  "batch_size": 32,
  "memory_capacity": 100000,
  "memory": {
      "type": "replay",
      "random_sampling": true
  },
  "update_frequency": 4,
  "first_update": 50000,
  "repeat_update": 1,

  "target_update_frequency": 10000,

  "discount": 0.99,
  "learning_rate": 0.00025,
  "optimizer": {
    "type": "rmsprop",
    "decay": 0.99,
    "epsilon": 1e-6
  },
  "tf_summary": null,
  "log_level": "info",

  "update_target_weight": 1.0,
  "double_dqn": false,
  "clip_loss": 1.0
}
